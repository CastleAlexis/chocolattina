{
  "name": "La Chocolattina Dev",
  "build": {
  "dockerfile": "Dockerfile",
  "context": "."
  },
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "bradlc.vscode-tailwindcss"
      ],
      "settings": {
        "editor.formatOnSave": true
      }
    }
  },
  "forwardPorts": [3000],
  "postCreateCommand": "pnpm install",
  "remoteUser": "node"
}
